{
    "name": "darthsoup/rundeck-php",
    "description": "rundeck php api",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Krummnacker",
            "email": "kevin.krummnacker@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "~1.3",
        "phpunit/phpunit": "~6.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "useable with guzzle 5 or 6"
    },
    "autoload": {
        "psr-4": {
            "DarthSoup\\Rundeck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarthSoup\\Rundeck\\Test\\": "tests/"
        }
    }
}
