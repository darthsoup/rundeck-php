{
    "name": "darthsoup/rundeck-php",
    "description": "PHP Rundeck API Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Krummnacker",
            "email": "kevin.krummnacker@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8 || ^9.4.2",
        "php-http/guzzle7-adapter": "*",
        "php-http/mock-client": "^1.2",
        "guzzlehttp/psr7": "^1.2",
        "phpstan/phpstan": "^0.12.23"
    },
    "suggest": {
        "guzzlehttp/guzzle": "use api with guzzleHttp"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DarthSoup\\Rundeck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarthSoup\\Rundeck\\Test\\": "tests/"
        }
    }
}
